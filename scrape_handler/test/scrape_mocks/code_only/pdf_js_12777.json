        "locked": false,
        "title": "Add support for finding/highlighting the outlineItem, corresponding to the currently visible page, in the sidebar (issue 7557, bug 1253820, bug 1499050)",
        "user": {
            "login": "Snuffleupagus",
            "id": 2692120,
            "node_id": "MDQ6VXNlcjI2OTIxMjA=",
            "avatar_url": "https://avatars.githubusercontent.com/u/2692120?v=4",
            "gravatar_id": "",
            "url": "https://api.github.com/users/Snuffleupagus",
