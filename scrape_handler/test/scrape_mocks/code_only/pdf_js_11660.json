        "title": "Fix Type1 font parsing when .notdef is not at index zero",
        "user": {
            "login": "janpe2",
            "id": 26090208,
            "node_id": "MDQ6VXNlcjI2MDkwMjA4",
            "avatar_url": "https://avatars.githubusercontent.com/u/26090208?v=4",
            "gravatar_id": "",
